<UserControl x:Class="ProjektBD.Asistant.AsistantSearchByCandidate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:wpfext="http://schemas.xceed.com/wpf/xaml/toolkit" 
             mc:Ignorable="d" 
             d:DesignHeight="213" d:DesignWidth="300">
    <StackPanel Name="mainPanel">
        <wpfext:WatermarkTextBox Width="100" Watermark="Podaj imie" Name="wtbName" Margin="20,20,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <wpfext:WatermarkTextBox Width="100" Watermark="Podaj nazwisko" Name="wtbSurname" Margin="20,20,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <wpfext:WatermarkTextBox Width="100" Watermark="Podaj PESEL" Name="wtbPesel" Margin="20,20,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        
        <Button Content="Szukaj" Height="23" Name="buttonSearch" Width="75" Margin="120,20,0,0" HorizontalAlignment="Left" Click="buttonSearch_Click" />
    </StackPanel>
</UserControl>
